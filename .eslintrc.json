{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "arrow-parens":"off",
        "brace-style": [
            "error",
            "allman"
        ],
        "comma-dangle": ["error", "never"],
        "indent": ["error", "2"],
        "require-jsdoc": "off"
    }
}
